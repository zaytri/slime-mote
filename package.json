{
  "name": "slime-mote",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "vercel-build": "npm run prisma:generate && npm run prisma:migrate:deploy && npm run build",
    "prisma:migrate:dev": "npx prisma migrate dev",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:seed": "npx prisma db seed",
    "prisma:generate": "npx prisma generate --schema=src/prisma/schema.prisma"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/node": "18.16.2",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.1",
    "clsx": "^1.2.1",
    "next": "13.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@types/sharp": "^0.32.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.1",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "prettier-plugin-prisma": "^4.12.0",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "prisma": "^4.13.0",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1"
  }
}
